{
  "manifest_version": 2,
  "name": "Search Engines Helper",
  "version": "1.2.0",
  "description": "Add custom search engines",
  "icons": {
    "128": "icons/128.png"
  },
  "options_ui": {
    "browser_style": true,
    "page": "settings.html"
  },
  "web_accessible_resources": ["search-template.xml"],
  "permissions": ["https://file.io/*", "notifications"],
  "applications": {
    "gecko": {
      "strict_min_version": "57.0"
    }
  }
}
